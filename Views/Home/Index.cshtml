@model HRMS_Silicon.ViewModel.IndexViewModel
@{ ViewData["Title"] = "Home Page"; }

<style>
    .Holiday-item {
        display: flex;
        flex-direction: column;
        height:350px;
        max-height: 350px;
        overflow-y: scroll
    }

    .Notice-Board-content {
        display: flex;
        flex-direction: column;
        height: 540px;
        overflow-y: scroll
    }

    .View-Notice {
        cursor: pointer;
    }

        .View-Notice:hover {
            background-color: #dddddd;
            border-radius: 5px;
        }


    .Subject {
        width: 160px;
        height: 16px;
        overflow: hidden;
    }

    .Description {
        font-size: 10px;
        width: 160px;
        height: 32px;
        overflow: hidden;
    }
</style>
<div class="row">
    <div class="col-xl-9 col-md-12 col-lg-12">
        <div class="row">
            <div class="col-xl-4 col-lg-6 col-md-12">
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-8">
                                <div class="mt-0 text-left">
                                    <span class="fs-14 font-weight-semibold">Total Employees</span>
                                    <h3 class="mb-0 mt-1 mb-2"><i class="feather feather-arrow-up-right mr-1 bg-success-transparent p-1 brround"></i>@Model.totalemployeelst.Count()</h3>
                                    @*<span class="text-muted">
                                        <span class="text-success fs-12 mt-2 mr-1"><i class="feather feather-arrow-up-right mr-1 bg-success-transparent p-1 brround"></i>124</span>
                                        for last month
                                    </span>*@
                                </div>
                            </div>
                            <div class="col-4">
                                <div class="icon1 bg-success my-auto  float-right"> <i class="feather feather-users"></i> </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xl-4 col-lg-6 col-md-12">
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-8">
                                <div class="mt-0 text-left">
                                    <span class="fs-14 font-weight-semibold">Designation</span>
                                    <h3 class="mb-0 mt-1 mb-2"><i class="feather feather-arrow-up-right mr-1 bg-success-transparent p-1 brround"></i>@Model.totaldesignationlst.Count()</h3>
                                    @*<span class="text-muted">
                                        <span class="text-danger fs-12 mt-2 mr-1"><i class="feather feather-arrow-down-left mr-1 bg-danger-transparent p-1 brround"></i>13</span>
                                        for last month
                                    </span>*@
                                </div>
                            </div>
                            <div class="col-4">
                                <div class="icon1 bg-primary my-auto  float-right"> <i class="feather feather-box"></i> </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xl-4 col-lg-12 col-md-12">
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            <div class="col-8">
                                <div class="mt-0 text-left">
                                    <span class="fs-14 font-weight-semibold">Expenses</span>
                                    <h3 class="mb-0 mt-1  mb-2">$2,7853</h3>
                                </div>
                                <span class="text-muted">
                                    <span class="text-danger fs-12 mt-2 mr-1"><i class="feather feather-arrow-up-right mr-1 bg-danger-transparent p-1 brround"></i>21.1% </span>
                                    for last month
                                </span>
                            </div>
                            <div class="col-4">
                                <div class="icon1 bg-secondary brround my-auto  float-right"> <i class="feather feather-dollar-sign"></i> </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xl-6 col-md-12 col-lg-6">
                <div class="card">
                    <div class="card-header border-0">
                        <h4 class="card-title">Up Comming Holidays</h4>
                    </div>
                    <div class="card-body">

                        <div class="Holiday-item">
                            <ul class="timeline">
                                @if (Model.holidaylst.Count > 0)
                                {
                                    foreach (var item in Model.holidaylst)
                                    {
                                        <li class="success" style="min-height:50px">
                                            <a class="font-weight-semibold fs-15 ml-3">@item.HolidayName (@item.HolidayDate.ToShortDateString())</a>
                                        </li>
                                    }

                                }
                                else
                                {
                                    <li class="success" style="min-height:50px">
                                        <a class="font-weight-semibold fs-15 ml-3">Upcomming Holidays will be published Soon! </a>
                                    </li>
                                }
                            </ul>

                        </div>

                    </div>
                </div>
            </div>
            <div class="col-xl-6 col-md-12 col-lg-6">
                <div class="card">
                    <div class="card-header border-0">
                        <h4 class="card-title">Recent Activity</h4>
                        <div class="card-options">
                            <div class="dropdown">
                                <a href="index.htm#" class="btn btn-outline-light" data-toggle="dropdown" aria-expanded="false"> View All <i class="feather feather-chevron-down"></i> </a>
                                <ul class="dropdown-menu dropdown-menu-right" role="menu">
                                    <li><a href="index.htm#">Monthly</a></li>
                                    <li><a href="index.htm#">Yearly</a></li>
                                    <li><a href="index.htm#">Weekly</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="card-body">
                        <ul class="timeline">
                            <li>
                                <a target="_blank" href="index.htm#" class="font-weight-semibold fs-15 ml-3">Leave Approval Request</a>
                                <a href="index.htm#" class="text-muted float-right fs-13">6 min ago</a>
                                <p class="mb-0 pb-0 text-muted pt-1 fs-11 ml-3">From "RuthDyer" UiDesign Leave</p>
                                <span class="text-muted  ml-3 fs-11"> On Monday 12 Jan 2020.</span>
                            </li>
                            <li class="primary">
                                <a target="_blank" href="index.htm#" class="font-weight-semibold fs-15 mb-2 ml-3">Wok Update</a>
                                <a href="index.htm#" class="text-muted float-right fs-13">10 min ago</a>
                                <p class="mb-0 pb-0 text-muted fs-11 pt-1 ml-3">From "Robert Marshall" Developer</p>
                                <span class="text-muted ml-3 fs-11">Task Completed.</span>
                            </li>
                            <li class="pink">
                                <a target="_blank" href="index.htm#" class="font-weight-semibold fs-15 mb-2 ml-3">Received Mail</a>
                                <a href="index.htm#" class="text-muted float-right fs-13">15 min ago</a>
                                <p class="mb-0 pb-0 text-muted fs-11 pt-1 ml-3">Emergency Sick Leave from "jacob Berry"</p>
                                <span class="text-muted ml-3 fs-11">Ui Designer, Designer Team.</span>
                            </li>
                            <li class="success mb-0 pb-0">
                                <a target="_blank" href="index.htm#" class="font-weight-semibold fs-15 mb-2 ml-3">Job Application Mail</a>
                                <a href="index.htm#" class="text-muted float-right fs-13">1 Hour ago</a>
                                <p class="mb-0 pb-0 text-muted fs-11 pt-1 ml-3">From jobmail@gmail.com laravel developer.</p>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xl-3 col-md-12 col-lg-12">
        <div class="card overflow-hidden">
            <div class="card-header border-0">
                <h4 class="card-title">Notice Board</h4>
            </div>
            <div class="pt-2">
                <div class="list-group Notice-Board-content">
                    @if (Model.noticelst.Count > 0)
                    {
                        foreach (var item in Model.noticelst)
                        {
                            <div class="list-group-item d-flex pt-3 pb-3 align-items-center border-0 View-Notice" id="@item.NoticeId">
                                <div class="mr-3 mr-xs-0">
                                    <div class="calendar-icon icons">
                                        <div class="date_time bg-info-transparent">
                                            <span class="date">@item.NoticeDate.Day</span>
                                            <span class="month">
                                                @HRMS_Silicon.Helper.DashboardHelper.getMonthNameByNumber(item.NoticeDate.Month)
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="ml-1">
                                    <div class="h5 fs-14 mb-1 Subject">
                                        @item.Subject
                                    </div>
                                    <div class="text-muted Description">
                                        @item.Description
                                    </div>
                                </div>
                            </div>
                        }
                    }
                    else
                    {
                        <div class="list-group-item d-flex pt-3 pb-3 align-items-center border-0 View-Notice">
                            <div class="mr-3 mr-xs-0">
                                <div class="calendar-icon icons">
                                    <div class="date_time bg-info-transparent">
                                        <span class="date">No</span>
                                        <span class="month">
                                            Data
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="ml-1">
                                <div class="h5 fs-14 mb-1 Subject">
                                    There are no new notice!
                                </div>
                                <div class="text-muted Description">
                                    No Data!!!
                                </div>
                            </div>
                        </div>
                    }

                </div>
            </div>
        </div>



    </div>


</div>



<div class="modal fade Notice-modal" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title ">Notice Board</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

<script src="~/lib/jquery/jquery.min.js"></script>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}

<script>
    $('.View-Notice').on('click', function () {
        let id = this.id;
        $.ajax({
            type: "GET",
            dataType: "html",
            url: "Notice/NoticePartial",
            data: { NoticeId: id },
            success: function (data) {
                debugger
                $('.modal-body').empty();
                $('.modal-body').append(data)
                $('.Notice-modal').modal('toggle');
            }
        })


    });
</script>





